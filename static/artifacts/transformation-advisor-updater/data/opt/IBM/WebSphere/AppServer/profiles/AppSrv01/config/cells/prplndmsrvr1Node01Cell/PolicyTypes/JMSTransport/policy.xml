<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wsp:Policy
        xmlns:wsjms="http://www.ibm.com/xmlns/prod/websphere/200801/ws-jmsTransport"
        xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy">
        <!-- Do not edit this file. -->
        <!-- This is the PolicyType for JMS transport -->
        <wsp:ExactlyOne>
            <wsp:All>
                <wsjms:requestTimeout>300</wsjms:requestTimeout>
                <wsjms:allowTransactionalAsyncMessaging>no</wsjms:allowTransactionalAsyncMessaging>
            </wsp:All>
        </wsp:ExactlyOne>
</wsp:Policy>