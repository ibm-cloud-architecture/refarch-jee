<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wsp:Policy
        xmlns:wshttp="http://www.ibm.com/xmlns/prod/websphere/200609/ws-httpTransport"
        xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy">
        <!-- Do not edit this file. -->
        <!-- This is the PolicyType for Https transport -->
        <wsp:ExactlyOne>
                <wsp:All>
                        <wshttp:outRequestSSLenabled>yes</wshttp:outRequestSSLenabled>
                        <wshttp:outAsyncResponseSSLenabled>no</wshttp:outAsyncResponseSSLenabled>
                        <wshttp:inResponseSSLenabled>yes</wshttp:inResponseSSLenabled>
                </wsp:All>
        </wsp:ExactlyOne>
</wsp:Policy>