<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE application PUBLIC "-//Sun Microsystems, Inc.//DTD J2EE Application 1.3//EN" "http://java.sun.com/dtd/application_1_3.dtd">

<application>
    <display-name>isclite</display-name>
    <description>Integrated Solutions Console</description>
    <module id="WebModule_1">
        <web>
            <web-uri>isclite.war</web-uri>
            <context-root>/ibm/console</context-root>
        </web>
    </module>
    <module id="WebModule_2">
        <web>
            <web-uri>adminredirector.war</web-uri>
            <context-root>/admin</context-root>
        </web>
    </module>
    <module id="WebModule_3">
        <web>
            <web-uri>iehs.war</web-uri>
            <context-root>/ibm/help</context-root>
        </web>
    </module>
    <module id="WebModule_4">
        <web>
            <web-uri>ISCAdminPortlet.war</web-uri>
            <context-root>/ISCAdminPortlet</context-root>
        </web>
    </module>
    <module id="WebModule_5">
        <web>
            <web-uri>WIMPortlet.war</web-uri>
            <context-root>/wim</context-root>
        </web>
    </module>
    <module id="WebModule_6">
        <web>
            <web-uri>wasportlet.war</web-uri>
            <context-root>/wasportlet</context-root>
        </web>
    </module>
    <security-role id="SecurityRole_1">
        <description>Administrator role</description>
        <role-name>administrator</role-name>
    </security-role>
    <security-role id="SecurityRole_2">
        <description>Operator role</description>
        <role-name>operator</role-name>
    </security-role>
    <security-role id="SecurityRole_3">
        <description>Configurator role</description>
        <role-name>configurator</role-name>
    </security-role>
    <security-role id="SecurityRole_4">
        <description>Monitor role</description>
        <role-name>monitor</role-name>
    </security-role>
</application>
