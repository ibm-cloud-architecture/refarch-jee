<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wsp:Policy xmlns:wsrmp="http://docs.oasis-open.org/ws-rx/wsrmp/200702" xmlns:wsp="http://www.w3.org/ns/ws-policy" xmlns="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd" xmlns:rmBinding="http://www.ibm.com/websphere/webservices/wsrmPolicyBinding" xmlns:rmPolicy="http://www.ibm.com/websphere/webservices/wsrmPolicySet">
    <wsp:ExactlyOne>
        <wsp:All>
            <wsrmp:RMAssertion>
                <rmPolicy:useMakeConnection>true</rmPolicy:useMakeConnection>
                <rmPolicy:inOrderDelivery>false</rmPolicy:inOrderDelivery>
